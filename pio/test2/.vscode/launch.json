    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/pio.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "device": "Pico2040",
            "configFiles": ["interface/picoprobe.cfg", "target/rp2040.cfg"],
            "searchDir":["F:/dev/pico/openocd/tcl"],
            "svdFile": "F:/dev/pico/pico-sdk/src/rp2040/hardware_regs/rp2040.svd",
        }
    ]
}